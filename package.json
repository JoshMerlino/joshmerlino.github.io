{
    "name": "vite-project",
    "version": "1.0.0",
    "description": "Create fast single-page progressive-web-apps using PhotonCSS 12, React and TypeScript.",
    "main": "lib/src",
    "types": "lib/src",
    "author": "Josh Merlino",
    "license": "ISC",
    "private": true,
    "scripts": {
        "preview": "vite preview",
        "dev": "vite --host",
        "build": "tsc -p tsconfig.node.json && tsc && vite build && cp public_html/index.html public_html/404.html",
        "deploy": "sudo rm -rf public_html && npm run build && sudo rsync -a --info=progress2 public_html/* /mnt/www-data/static/$(basename $PWD)"
    },
    "devDependencies": {
        "@types/node": "^18.15.5",
        "@types/react": "^18.0.29",
        "@types/react-dom": "^18.0.11",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "@vitejs/plugin-react": "^3.1.0",
        "apexcharts": "^3.37.2",
        "autoprefixer": "^10.4.14",
        "chokidar": "^3.5.3",
        "classnames": "^2.3.2",
        "concurrently": "^7.6.0",
        "dayjs": "^1.11.6",
        "eslint": "^8.36.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "lessc": "^1.0.2",
        "nodemon": "^2.0.22",
        "react": "^18.1.0",
        "react-apexcharts": "^1.4.0",
        "react-discord-invite": "^4.0.8",
        "react-dom": "^18.2.0",
        "react-fast-marquee": "^1.3.5",
        "react-query": "^3.39.3",
        "react-router-dom": "^6.9.0",
        "tailwindcss": "^3.2.7",
        "typescript": "^5.0.2",
        "vite": "^4.2.1",
        "vite-plugin-html-config": "^1.0.11",
        "vite-plugin-pwa": "^0.14.6",
        "web-manifest-reader": "^1.2.3"
    }
}